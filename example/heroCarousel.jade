.heroCarousel
  .heroCarouselWindow
    ul
      - for (var i = 1; i < 6; ++i) {
      li
        .heroCarouselImage
          a(href='replaceMe')
            img(data-src='example.com/938x298/mo')
        .heroCarouselContent.flip
          .heroCarouselContentInner
            .heroCarouselHeader @Html.Ipsum.h4(4)
            .heroCarouselText @Html.Ipsum.p(1, 3)
            .heroCarouselButton: a(href='') View Now
      - }